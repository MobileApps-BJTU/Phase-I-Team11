<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xx.administrator.calendar1" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EventList" >
        </activity>
        <activity
            android:name=".AddEventActivity"
            android:label="@string/title_activity_add_event" >
        </activity>
        <activity
            android:name=".RepeatActivity"
            android:label="@string/title_activity_repeat" >
        </activity>
        <activity
            android:name=".AlertTimeActivity"
            android:label="@string/title_activity_alert_time" >
        </activity>
        <activity
            android:name=".RemindTypeActivity"
            android:label="@string/title_activity_remind_type" >
        </activity>
    </application>

</manifest>
